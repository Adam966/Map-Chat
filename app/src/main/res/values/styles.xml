<resources>

    <!-- Base application theme. -->
    <style name="AppNormal" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="bkgcolor">#FFFFFF</item>
        <item name="barcolor">@color/colorPrimary</item>
        <item name="iconcolor">#FFFFFF</item>
        <item name="coloriconchecked">#000000</item>
        <item name="textcolor">#FFFFFF</item>
    </style>

    <style name="AppDark" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="bkgcolor">#4B4B4C</item>
        <item name="barcolor">#000000</item>
        <item name="iconcolor">#FFFFFF</item>
        <item name="coloriconchecked">#4B4B4C</item>
        <item name="textcolor">#FFFFFF</item>
    </style>

    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_bkg</item>
    </style>

    <style name="AppNormal.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppNormal.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppNormal.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>
